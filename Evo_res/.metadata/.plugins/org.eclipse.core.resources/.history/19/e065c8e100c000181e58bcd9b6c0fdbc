package refactorIPO;

public class Personne {
	
	public Personne(String nom, int numeroRue, String nomRue, String ville, int codePostal, String pays) {
		super();
		this.nom = nom;
		this.numeroRue = numeroRue;
		this.nomRue = nomRue;
		this.ville = ville;
		this.codePostal = codePostal;
		this.pays = pays;
	}

	String nom;
	
	int numeroRue;
	String  nomRue;
	String ville;
	int codePostal;
	String pays;

}
