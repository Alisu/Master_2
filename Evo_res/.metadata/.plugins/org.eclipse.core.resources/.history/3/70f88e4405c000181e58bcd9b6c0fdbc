package refactorIPO;

public class Personne {
	
	String nom;
	
	int numeroRue;
	String  nomRue;
	String ville;
	int codePostal;
	String pays;
	
	public Personne(String nom, int numeroRue, String nomRue, String ville, int codePostal, String pays) {
		super();
		this.nom = nom;
		this.numeroRue = numeroRue;
		this.nomRue = nomRue;
		this.ville = ville;
		this.codePostal = codePostal;
		this.pays = pays;
	}
	
	public void MoveIn(int numeroRue, String nomRue, String ville, int codePostal, String pays){
		this.numeroRue = numeroRue;
		this.nomRue = nomRue;
		this.ville = ville;
		this.codePostal = codePostal;
		this.pays = pays;
	}
	
	public static void main(String[] args){
		Etudiant p = new Etudiant("Michel", 17, "rue des orchid√©s", "Pignan", 34540,"France",20112018);
	}

}
